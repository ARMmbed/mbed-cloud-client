stage('Trigger Branch Build') {
script {
        echo "Triggering job "
    // print all params
		def str = 'Build params:\n'
		env.getEnvironment().each { name, value -> str = str + "\t$name = $value\n" }
		echo str				
	node {
	checkout scm
	}
        build job: "QE_SE_test/QE_FCCE_SE_production_mode", wait: false, parameters: [string(name: 'pull_request', value: "PULL_REQUEST_TRUE"),
	string(name: 'pr_id', value: "${CHANGE_ID}"),string(name: 'change_url', value: "${CHANGE_URL}")]
}
}
